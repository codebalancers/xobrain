{
  "appId": "com.codebalancers.xobrain",
  "productName": "xobrain",
  "directories": {
    "output": "release-builds"
  },
  "files": [
    "www/**/*",
    "main.js",
    "migrations/*.js"
  ],
  "win": {
    "icon": "www/assets/icon",
    "target": [
      "portable"
    ]
  },
  "mac": {
    "icon": "www/assets/icon",
    "target": [
      "dmg"
    ]
  },
  "linux": {
    "icon": "www/assets/icon",
    "target": [
      "AppImage"
    ]
  }
}
